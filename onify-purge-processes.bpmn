          <bpmn:incoming>Flow_0vovp9i</bpmn:incoming>
          <bpmn:outgoing>Flow_0gokgh2</bpmn:outgoing>
          <bpmn:outgoing>Flow_0x3heb9</bpmn:outgoing>
        </bpmn:exclusiveGateway>
